{
  "name": "Unofficial Redfin Extension",
  "version": "1.0",
  "description": "Adds missing links to Redfin listings.",
  "content_scripts": [
    {
      "matches": ["http://www.redfin.com/*"],
      "js": ["jquery-1.4.2.min.js", "contentscript.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "http://www.redfin.com/*"
  ]
}
